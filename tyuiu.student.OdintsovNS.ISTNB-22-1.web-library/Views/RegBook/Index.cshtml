@model RegBook
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    var person = ViewBag["Person"];
}
<h1>RegBook</h1>
<div>
    <ul>
        @foreach(var book in Model.Books)
        {
            <li>
                @book.Name
            </li>
        }
    </ul>
</div>
<div>
    <ul>
        @foreach (var author in Model.Rooms)
        {
            <li>
                @author.Number
            </li>
        }
    </ul>
</div>
<div>
    <ul>
        @foreach (var author in person)
        {
            <li>
                @author.Name
            </li>
        }
    </ul>
</div>